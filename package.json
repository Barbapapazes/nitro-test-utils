{
  "name": "nitro-test-utils",
  "type": "module",
  "version": "0.3.1",
  "packageManager": "pnpm@8.15.6",
  "description": "Test utilities for Nitro",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/nitro-test-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nitro-test-utils.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/nitro-test-utils/issues"
  },
  "keywords": [
    "nitro",
    "nitropack",
    "nuxt"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./config": {
      "types": "./dist/config.d.mts",
      "import": "./dist/config.mjs"
    },
    "./e2e": {
      "types": "./dist/e2e.d.mts",
      "import": "./dist/e2e.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "prepare": "pnpm run build && pnpm -C test/fixture prepare"
  },
  "peerDependencies": {
    "nitropack": "^2",
    "vitest": "^1"
  },
  "dependencies": {
    "listhen": "^1.7.2",
    "ofetch": "^1.3.4",
    "pathe": "^1.1.2",
    "ufo": "^1.5.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.3",
    "@types/node": "^20.12.7",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "nitropack": "^2.9.6",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.8",
    "vitest": "^1.4.0"
  }
}
